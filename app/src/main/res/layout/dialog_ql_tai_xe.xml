<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <EditText
        android:id="@+id/ed_tenTX_dialog"
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"/>
    <EditText
        android:id="@+id/ed_tuoiTX_dialog"
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"/>
    <EditText
        android:id="@+id/ed_sdtTX_dialog"
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"/>
    <EditText
        android:id="@+id/ed_tenDNTX_dialog"
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"/>
    <EditText
        android:id="@+id/ed_matKhauTX_dialog"
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnCancle_TX"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="cancle"/>
        <Button
            android:id="@+id/btnOK_TX"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ok"/>

    </LinearLayout>
</LinearLayout>